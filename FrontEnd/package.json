    "preview:mahar.admin": "vite preview --config vite.config.mahar.adminpanel.ts --port 5173",
    "preview:mahar.website": "vite preview --config vite.config.mahar.website.ts --port 5174",

    
    "dev:pmis": "vite --config vite.config.pmis.ts --mode pmis.development",
